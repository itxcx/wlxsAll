.memberPoints{
  width: 100%;
  height: 100vh;
  background: #66D173;
}
.pointHeader{
  width: 100vw;
  height: 30vh;
  position: relative;
  color: #ffffff;
}
.pointHeader image{
  width: 100vw;
  height: 30vh;
  position: absolute;
  top: 0;
  left: 0;
}
.pointHeader view{
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  top: 7vh;
}
.pointHeader view text:nth-of-type(1){
  display: block;
  font-size: 1.6rem;
  font-weight: 600;
}
.pointHeader view text:nth-of-type(2){
  display: block;
  font-size: 0.6rem;
}
.pointHeader>text{
  font-size: 0.8rem;
  width: 100%;
  display: block;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 2vh; 
}
.pointContent{
  height: 64vh;
  width: 88vw;
  margin: -0.2vh 3vw 2vh 3vw;
  background: #ffffff;
  border-radius: 5px;
  overflow: auto;
  padding: 2vh 3vw;
}
.pointsItem{
  border-bottom: 1px solid #F2F2F2;
  padding: 2vh 0;
  overflow: hidden;
  height: 8vh;
}
.getType{
  width: 70vw;
  float: left;
}
.getType text:nth-of-type(1){
  display: block;
  font-size: 0.95rem;
}
.getType text:nth-of-type(2){
  display: block;
  margin-top: 1vh;
  font-size: 0.7rem;
  color: #B2B2B2;
}
.pointNum{
  width: 10vw;
  height: 8vh;
  line-height: 8vh;
  float: right;
  color: #66D173;
}