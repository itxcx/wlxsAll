.show{
  display: block;
}
.hide{
  display: none;
}

.openDoor{
  width: 100vw;
  height: 100vh;
}
/*  openning  */
.openning, .openSuccess{
  margin-top: 4vh;
  text-align: center;
}
.openning image, .openSuccess image{
  width: 35vh;
  height: 35vh;
}
.openningContent, .openSuccessContent{
  margin-top: 5vh;
  font-size: 1.2rem;
}
.tipContent{
  margin: 4vh 0;
  width: 100vw;
  text-align: center;
   color: #d3d3d3;
  font-size: 0.95rem;
}
.tipContent .title{
  display: block;
  text-align: center;
  font-size: 1rem;
  margin: 5vh 0 2vh 0;
  color: #000000;
}
.tipContent .items{
  display: block;
}
.bottomBanner{
  width: 94vw;
  height: 20vh;
  position: fixed;
  bottom: 1vh;
  left: 3vw;
}
.bottomBanner image{
  width: 94vw;
  height: 20vh;
}
/* account  */
.account, .accountFail{
  margin-top: 4vh;
  text-align: center;
  border-bottom: 10px solid #f2f2f2;
  padding-bottom: 4vh;
}
.accountImg, .accountFailImg{
  width: 100vw;
  text-align: center;
}
.accountImg image, .accountFailImg image{
  width: 10vh;
  height: 11vh;
}
.detailInfo text:nth-of-type(1){
    display: block;
    font-size: 1.5rem;
}
.detailInfo text:nth-of-type(2){
    display: block;
    font-size: 0.95rem;
    color: #000000;
}
.detailInfo text:nth-of-type(3){
    display: block;
    font-size: 0.95rem;
    color: #66D173;
}
.detailInfo text:nth-of-type(4){
  display: block;
  font-size: 0.95rem;
  color: #d3d3d3;
  margin: 2vh 0;
}
.accountBtn, .accountFailBtn{
  margin-top: 8vh;
}
.accountBtn text, .accountFailBtn text{
    display: inline-block;
    padding: 1vh 8vw;
    border: 1px solid #66D173;
    border-radius: 20px;
    font-size: 0.75rem;
    color: #66D173;
}
.accountBtn text:nth-of-type(2) {
  margin-left: 10vw;
}
/* error  */
.openError{
  width: 100vw;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
}
.errorImg{
  margin-top: 5vh;
  width: 100vw;
  height: 35vh;
  text-align: center;
}
.errorImg image{
  height: 35vh;
  width: 35vh;
}
.errorContent{
  padding: 3vh 5vw;
  text-align: center;
}
.contentTitle{
  display: block;
  font-size: 1.2rem;
  margin-bottom: 3vh;
}
.contentItem{
  display: block;
  font-size: 0.85rem;
}
.openAgain{
  width: 100vw;
  height: 20vh;
  text-align: center;
  margin-top: 4vh;
}
.openAgain image{
  width: 20vh;
  height: 20vh;
}
.repair{
  width: 100vw;
  margin-top: 5vh;
  text-align: center;
}
.repair text{
  display: block;
  border: 1px solid #66D173;
  width: 30vw;
  margin-left: 35vw;
  padding: 1vh 0;
  border-radius: 20px;
  color: #66D173;
  font-size: 0.85rem;
}
/* 未支付订单  */
.unpaid{
  width: 100vw;
  height: 100vh;
}
.unpaidImg {
  width: 100vw;
  height: 35vh;
  margin-top: 5vh;
  text-align: center;
}
.unpaidImg image{
  height: 35vh;
  width: 35vh;
}
.unpaidContent text:nth-of-type(1){
  display: block;
  text-align: center;
  font-size: 1.2rem;
  margin-top: 4vh;
}
.unpaidContent text:nth-of-type(2){
  display: block;
  text-align: center;
  font-size: 0.75rem;
  margin-top: 2vh;
}
.checkDetail{
  width: 100vw;
  text-align: center;
  margin-top: 8vh;
}
.checkDetail text{
  display: block;
  width: 50vw;
  margin-left: 25vw;
  background: #66D173;
  padding: 1vh 0;
  color: #ffffff;
  border-radius: 20px;
  font-size: 0.85rem;
}
.alipay{
  width: 100%;
  text-align: center;
  font-size: 0.95rem;
  margin-top: 2vh;
}
/* modal style  */
.modalLoading{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.5);
  z-index: 1000;
}
.spinner {
  margin:  auto;
  width: 10vw;
  height: 10vw;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
}
 
.container1 > view, .container2 > view, .container3 > view {
  width: 6px;
  height: 6px;
  background-color: #66D173;
  border-radius: 100%;
  position: absolute;
  -webkit-animation: bouncedelay 1.2s infinite ease-in-out;
  animation: bouncedelay 1.2s infinite ease-in-out;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
 
.spinner .spinner-container {
  position: absolute;
  width: 100%;
  height: 100%;
}
 
.container2 {
  -webkit-transform: rotateZ(45deg);
  transform: rotateZ(45deg);
}
 
.container3 {
  -webkit-transform: rotateZ(90deg);
  transform: rotateZ(90deg);
}
 
.circle1 { top: 0; left: 0; }
.circle2 { top: 0; right: 0; }
.circle3 { right: 0; bottom: 0; }
.circle4 { left: 0; bottom: 0; }
 
.container2 .circle1 {
  -webkit-animation-delay: -1.1s;
  animation-delay: -1.1s;
}
 
.container3 .circle1 {
  -webkit-animation-delay: -1.0s;
  animation-delay: -1.0s;
}
 
.container1 .circle2 {
  -webkit-animation-delay: -0.9s;
  animation-delay: -0.9s;
}
 
.container2 .circle2 {
  -webkit-animation-delay: -0.8s;
  animation-delay: -0.8s;
}
 
.container3 .circle2 {
  -webkit-animation-delay: -0.7s;
  animation-delay: -0.7s;
}
 
.container1 .circle3 {
  -webkit-animation-delay: -0.6s;
  animation-delay: -0.6s;
}
 
.container2 .circle3 {
  -webkit-animation-delay: -0.5s;
  animation-delay: -0.5s;
}
 
.container3 .circle3 {
  -webkit-animation-delay: -0.4s;
  animation-delay: -0.4s;
}
 
.container1 .circle4 {
  -webkit-animation-delay: -0.3s;
  animation-delay: -0.3s;
}
 
.container2 .circle4 {
  -webkit-animation-delay: -0.2s;
  animation-delay: -0.2s;
}
 
.container3 .circle4 {
  -webkit-animation-delay: -0.1s;
  animation-delay: -0.1s;
}
 .loadingText{
   position: fixed;
   bottom: 40vh;
   left: 0;
   color: #66D173;
   width: 100vw;
   text-align: center;
   font-size: 0.95rem;
 }
@-webkit-keyframes bouncedelay {
  0%, 80%, 100% { -webkit-transform: scale(0.0) }
  40% { -webkit-transform: scale(1.0) }
}
 
@keyframes bouncedelay {
  0%, 80%, 100% {
    transform: scale(0.0);
    -webkit-transform: scale(0.0);
  } 40% {
    transform: scale(1.0);
    -webkit-transform: scale(1.0);
  }
}
.show{
  display: block;
}
.hide{
  display: none;
}