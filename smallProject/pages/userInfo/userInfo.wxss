.userInfo{
  width: 100vw;
  margin: 0;
  padding: 0;
}
.userInfoHeader{
  width: 100vw;
  height: 30vh;
  background: #66D173;
  padding-top: 5vh;
  position: relative;
}
.userInfoHeader image{
  width: 100%;
  height: 30vh;
  position: absolute;
  left: 0;
  top: 5vh;
  z-index: 1;
}
.userImg{
  text-align: center;
  position: absolute;
  width: 100%;
  top: 1vh;
  left: 0; 
  z-index: 2;
}
.userImg image{
  width: 10vh;
  height: 10vh;
  margin: 0 auto;
  border-radius: 50%;
  display: block;
}
